---
---
[
  {% for type in site.types %}
  {% unless type.excluded == true %}
    {
      "title"    : "{{ type.title | escape }}",
      "category" : "{{ type.categories | join: ' ' }}",
      "content"	 : {{ type.content | markdownify | strip_html | strip_newlines | remove:  "	" | escape | remove: "\" | json }},
      "href"     : "{{ site.baseurl }}{{ type.url }}",
      "date"     : "{{ type.date }}"
    },
  {% endunless %}
  {% endfor %}
  {% for product in site.data.product_condo %}
    {
      "title"    : "{{ product.title | escape }}",
      "content"	 : {{ product.address | markdownify | strip_html | strip_newlines | remove:  "	" | escape | remove: "\" | json }},
      "href"     : "{{ site.baseurl }}/window-replacement/condominium/{{ product.url }}/"
    },
  {% endfor %}
  {% for range in site.product_westag %}
    {
      "title"    : "{{ range.title | escape }}",
      "content"	 : {{ range.content | markdownify | strip_html | strip_newlines | remove:  "	" | escape | remove: "\" | json }},
      "href"     : "{{ site.baseurl }}/type/westag-getalit-premium-interior-doors/{{ range.category }}/"
    },
  {% endfor %}
  {% for page in site.pages %}
  {% unless page.excluded == true or page.url contains 'sitemap.xml' or page.url contains 'feed.xml' %}
  {% unless page.content contains "Window replacement should be quick and easy" %}
    {
      "title"    : "{{ page.title | escape }}",
      "category" : "{{ page.categories | join: ' ' }}",
      "content"	 : {{ page.content | markdownify | strip_html | strip_newlines | remove:  "	" | escape | remove: "\" | json }},
      "href"     : "{{ site.baseurl }}{{ page.url }}",
      "date"     : "{{ page.date }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endunless %}
  {% endunless %}
  {% endfor %}
]
